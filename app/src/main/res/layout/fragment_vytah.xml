<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".VytahFragment">

    <TextView
        android:id="@+id/vytahNadpis"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="@string/vytah"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.334"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/floatingActionButtonAdresyVytah"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="16dp"
        android:clickable="true"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@android:drawable/ic_menu_preferences" />

    <ImageView
        android:id="@+id/imageViewVytahHore"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="32dp"
        android:layout_marginBottom="412dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/baseline_circle_24" />

    <ImageView
        android:id="@+id/imageViewDrahaVzduch"
        android:layout_width="65dp"
        android:layout_height="63dp"
        android:layout_marginBottom="104dp"
        android:rotation="-90"
        app:layout_constraintBottom_toBottomOf="@+id/imageDraha"
        app:layout_constraintEnd_toEndOf="@+id/imageDraha"
        app:layout_constraintHorizontal_bias="0.46"
        app:layout_constraintStart_toStartOf="@+id/imageDraha"
        app:srcCompat="@drawable/baseline_air_24" />

    <ImageView
        android:id="@+id/imageViewVytahDole"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="32dp"
        android:layout_marginBottom="228dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/baseline_circle_24" />

    <androidx.constraintlayout.widget.Group
        android:id="@+id/group"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <LinearLayout
        android:id="@+id/linearLayout2"
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginTop="64dp"
        android:layout_weight="1"
        android:orientation="vertical"
        app:layout_constraintStart_toEndOf="@+id/linearLayout"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="32dp"
            android:layout_marginBottom="8dp"
            android:text="Výstupy"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/q_vytah_d"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="8dp"
            android:text="Výťah D" />

        <TextView
            android:id="@+id/q_vytah_h"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="8dp"
            android:text="Výťah H" />

        <TextView
            android:id="@+id/q_piest"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="8dp"
            android:text="Piest" />

        <TextView
            android:id="@+id/q_draha"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="8dp"
            android:text="Dráha" />

        <TextView
            android:id="@+id/q_manual"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="8dp"
            android:text="Režim Manual" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="64dp"
        android:orientation="vertical"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="32dp"
            android:layout_marginBottom="8dp"
            android:text="  Vstupy"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/i_snimac_l"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="8dp"
            android:text="Snímač L" />

        <TextView
            android:id="@+id/i_snimac_p"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="8dp"
            android:text="Snímač P" />

        <TextView
            android:id="@+id/i_rameno_pritomne"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="8dp"
            android:text="Rameno Prítomné" />

        <TextView
            android:id="@+id/i_vytah_h"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="8dp"
            android:text="Výťah H" />

        <TextView
            android:id="@+id/i_vytah_d"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="8dp"
            android:text="Výťah D" />

        <TextView
            android:id="@+id/i_snimac_h"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="8dp"
            android:text="Snímač H" />

        <TextView
            android:id="@+id/i_piest_vysunuty"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="8dp"
            android:text="Piest Vysunutý" />
    </LinearLayout>

    <TextView
        android:id="@+id/errorTextVytah"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="Errory"
        app:layout_constraintStart_toStartOf="@+id/linearLayout"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout" />

    <com.google.android.material.materialswitch.MaterialSwitch
        android:id="@+id/switchReadVytah"
        android:layout_width="166dp"
        android:layout_height="51dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="12dp"
        android:text="Monitorovanie"
        app:layout_constraintStart_toEndOf="@+id/linearLayout"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout2" />

    <Button
        android:id="@+id/buttonVytahHore"
        android:layout_width="120dp"
        android:layout_height="50dp"
        android:layout_marginBottom="8dp"
        android:text="Výťah hore"
        app:layout_constraintBottom_toTopOf="@+id/buttonVytahDole"
        app:layout_constraintStart_toStartOf="@+id/buttonVytahDole" />

    <Button
        android:id="@+id/buttonPiest"
        android:layout_width="120dp"
        android:layout_height="50dp"
        android:layout_marginBottom="8dp"
        android:text="Piest"
        app:layout_constraintBottom_toTopOf="@+id/buttonDraha"
        app:layout_constraintEnd_toEndOf="@+id/buttonDraha" />

    <Button
        android:id="@+id/buttonDraha"
        android:layout_width="120dp"
        android:layout_height="50dp"
        android:layout_marginEnd="64dp"
        android:layout_marginBottom="16dp"
        android:text="Dráha"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/buttonVytahDole" />

    <Button
        android:id="@+id/buttonVytahDole"
        android:layout_width="120dp"
        android:layout_height="50dp"
        android:layout_marginStart="64dp"
        android:layout_marginBottom="16dp"
        android:text="Výťah dole"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/buttonDraha"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/buttonAutoRez"
        android:layout_width="96dp"
        android:layout_height="51dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="20dp"
        android:text="Auto"
        app:layout_constraintStart_toEndOf="@+id/linearLayout2"
        app:layout_constraintTop_toTopOf="@+id/linearLayout2" />

    <Button
        android:id="@+id/buttonManualRez"
        android:layout_width="96dp"
        android:layout_height="51dp"
        android:layout_marginTop="12dp"
        android:text="Manual"
        app:layout_constraintEnd_toEndOf="@+id/buttonAutoRez"
        app:layout_constraintTop_toBottomOf="@+id/buttonAutoRez" />

    <ImageView
        android:id="@+id/imagePiestVysuvanie"
        android:layout_width="128dp"
        android:layout_height="128dp"
        android:layout_marginEnd="14dp"
        android:src="@drawable/piestvysun"
        app:layout_constraintEnd_toEndOf="@+id/imagePiest"
        app:layout_constraintTop_toTopOf="@+id/imagePiest" />

    <ImageView
        android:id="@+id/imageSuciastka"
        android:layout_width="128dp"
        android:layout_height="128dp"
        android:layout_marginEnd="100dp"
        android:src="@drawable/puk"
        app:layout_constraintEnd_toEndOf="@+id/imagePiestVysuvanie"
        app:layout_constraintTop_toTopOf="@+id/imagePiestVysuvanie" />

    <ImageView
        android:id="@+id/imageViewSnimacL"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="76dp"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toTopOf="@+id/buttonVytahHore"
        app:layout_constraintStart_toStartOf="@+id/buttonVytahHore"
        app:srcCompat="@drawable/baseline_circle_24" />

    <ImageView
        android:id="@+id/imageViewSnimacP"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageViewSnimacL"
        app:layout_constraintStart_toEndOf="@+id/imageViewSnimacL"
        app:layout_constraintTop_toTopOf="@+id/imageViewSnimacL"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/baseline_circle_24" />

    <ImageView
        android:id="@+id/imageDraha"
        android:layout_width="128dp"
        android:layout_height="128dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="48dp"
        android:src="@drawable/draha"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/errorTextVytah" />

    <ImageView
        android:id="@+id/imagePiest"
        android:layout_width="128dp"
        android:layout_height="128dp"
        android:layout_marginEnd="12dp"
        android:layout_marginBottom="52dp"
        android:src="@drawable/piestik"
        app:layout_constraintBottom_toTopOf="@+id/buttonPiest"
        app:layout_constraintEnd_toStartOf="@+id/imageViewVytahDole" />

</androidx.constraintlayout.widget.ConstraintLayout>